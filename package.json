    "vuetify": "^3.7.0-beta.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "concurrently": "^9.1.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "vue-tsc": "^2.1.10"
  }
